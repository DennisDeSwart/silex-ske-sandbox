        "doctrine/dbal": "~2.3",
        "monolog/monolog": "~1.6",
        "twig/twig": "~1.10",
        "twig/extensions": "~1.3",
        "symfony/framework-bundle": "~2.3",
        "symfony/config": "~2.3",
        "symfony/console": "~2.3",
        "symfony/form": "~2.3",
        "symfony/finder": "~2.2",
        "symfony/security": "~2.3",
        "symfony/translation": "~2.3",
        "symfony/twig-bridge": "~2.3",
        "symfony/validator": "~2.3",
        "symfony/yaml": "~2.3",
        "igorw/config-service-provider": "~1.2",
        "marcojanssen/silex-service-register-provider": "~1.1",
        "dflydev/doctrine-orm-service-provider": "1.0.6"
