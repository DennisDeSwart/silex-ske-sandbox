{
    "name"        : "ooXei1sh/silex-ske-sandbox",
    "type"        : "sandbox",
    "description" : "A Silex Edition based on http://lyrixx.github.com/Silex-Kitchen-Edition.",
    "license"     : "UNLICENSE",
    "require": {
        "silex/silex": "~1.1",
